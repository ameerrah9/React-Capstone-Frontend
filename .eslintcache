        value={ signupFormData.username}\n              name=\"username\"\n              placeholder=\"username\"\n              onChange={ handleInputChange} />\n    </Form.Group>\n\n    <Form.Group controlId=\"formBasicEmail\">\n            <Form.Control  \n              type=\"text\" \n              value={ signupFormData.email}\n              name=\"email\"\n              placeholder=\"email\"\n              onChange={ handleInputChange} />\n    </Form.Group>\n\n              <Form.Group>\n            <Form.Control  \n              type=\"text\" \n              value={ signupFormData.img_src}\n              name=\"img_src\"\n              placeholder=\"profile photo\"\n              onChange={ handleInputChange} />\n    </Form.Group>\n\n    <Form.Group controlId=\"formBasicPassword\">\n          <Form.Control  \n            type=\"password\" \n            value={ signupFormData.password}\n            name=\"password\"\n            placeholder=\"password\"\n            onChange={ handleInputChange } />\n  </Form.Group>\n\n          <Button variant=\"primary\" value=\"Signup\" type=\"submit\">Signup</Button>\n      </Form>\n  );\n}\n\nconst mapStateToProps = state => {\n  return {\n    signupFormData: state.signupForm\n  }\n}\n\nexport default connect(mapStateToProps, { updateSignupForm, signup } )(Signup)\n","/Users/GhameerahM/Learn/code/capstone-react-app/game-zone-frontend/src/components/Home.js",["199"],"import React from 'react';\nimport { Link } from 'react-router-dom'\n\n\nconst Home = () => (\n  <div>\n  <h3>WELCOME TO GAMEZONE</h3>\n  <p>Where NBA fans everywhere can unite.</p>\n    <span>\n      <Link to=\"/signup\">Sign Up</Link> or <Link to=\"/login\">Log In</Link>\n    </span>\n    <br></br>\n    <img width={100} height={200} src=\"https://upload.wikimedia.org/wikipedia/en/thumb/0/03/National_Basketball_Association_logo.svg/210px-National_Basketball_Association_logo.svg.png\" />\n  </div>\n\n);\n\nexport default Home;","/Users/GhameerahM/Learn/code/ca